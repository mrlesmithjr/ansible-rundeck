{{ ansible_managed | comment }}
loglevel.default=INFO
rdeck.base={{ rundeck_base }}

rss.enabled=false

grails.serverURL={{ rundeck_grails_serverURL }}

dataSource.dbCreate = update

dataSource.url = jdbc:h2:file:{{ rundeck_base }}/data/rundeckdb;MVCC=true;TRACE_LEVEL_FILE=4

rundeck.log4j.config.file = {{ rundeck_root_dir + '/log4j.properties' }}

{% if rundeck_gui_customization is defined and rundeck_gui_customization %}
{%   for pair in rundeck_gui_customization %}
{{ pair.name }} = {{ pair.value }}
{%   endfor %}
{% endif %}
